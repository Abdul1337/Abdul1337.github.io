{
  "manifest_version": 2,

  "name": "Gravitify",
  "description": "Enable Gravitify and see the magic",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon_20.png",
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["content.js"]
    }
  ]

}
